            <a class="list-group-item list-group-item-action" id="list-profile-list">Profile</a>
            <a class="list-group-item list-group-item-action" id="list-messages-list">Messages</a>
            <a class="list-group-item list-group-item-action" id="list-settings-list">Settings</a>
        </div>
    </div>
    <div class="col-8">
        <div class="tab-content" id="nav-tabContent">
            <div class="tab-pane fade show active" id="list-home" v-for="list in lists" >Home</div>
            {list.title}
        </div>
    </div>
    <div class="col-12">
        <div class="tab-content" id="nav-tabContent">
            <div class="tab-pane fade show active" id="list-home" >Home</div>
            <div class="tab-pane fade" id="list-profile">Profile</div>
            <div class="tab-pane fade" id="list-messages">Messages</div>
